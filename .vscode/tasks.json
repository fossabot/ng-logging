{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "lint",
      "problemMatcher": {
        "base": "$tslint5",
        "fileLocation": "relative"
      }
    },
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/samples/ng-logging-aspnetcore-sample/ng-logging-aspnetcore-sample.csproj"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
